#import "HOLMapPoint.h"

@implementation HOLMapPoint

@synthesize coordinate;
@synthesize locID;
@synthesize pointType;

- (void)dealloc {
	[self.locID release];
    [super dealloc];
}

// Public functions
- (id)initWithCoordinates:(CLLocationCoordinate2D)coordinates ptType:(HOLMAPPOINTTYPE)ptType {
	self.pointType = ptType;
	self.coordinate = coordinates;
	
	return self;
}

@end
